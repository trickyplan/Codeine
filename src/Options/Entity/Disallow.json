{
    "Hooks":
    {
        "beforeDisallowDo":
        {
            "Load model (essential)":
            {
                "Service":"Entity",
                "Method":"Load"
            }
        },
        "beforeDisallowGet":
        {
            "Store URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "StoreURL"
            }
        },
        "afterDisallowPost":
        {
            "Restore URL":
            {
                "Service": "System.Interface.HTTP",
                "Method": "RestoreURL"
            }
        }
    }
}