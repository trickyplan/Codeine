{
    "Context": "",
    "HTTP":
    {
        "Headers":
        {
            "Content-Type:": "text/html; charset=utf-8"
        }
    },
    "View":
    {
        "HTML":
        {
            "Widget Set": "bootstrap-codeine",
            "Title": [],
            "Keywords": [],
            "Description": []
        }
    },
    "Extension": ".html",
    "Layout": "<place>Content</place>",
    "Place Pattern": "<place>(.*?)<\/place>",
    "Hooks":
    {
        "beforeHTMLPipeline":
        {
            "Layouts":
            {
                "Service":"View.HTML.Layouts",
                "Method":"Do"
            }
        },
        "afterHTMLPipeline":
        {
            "Call":
            {
                "Service": "View.Hooks.Call",
                "Method": "Parse",
                "Call":
                {
                    "Remove empty": true,
                    "Value Key": "Output"
                }
            }
        },
        "afterHTMLRender":
        {
            "Parslets":
            {
                "Service":"View.HTML.Parslets",
                "Method":"afterHTMLRender"
            },
            "Images":
            {
                "Service":"View.HTML.Image",
                "Method":"Process Page"
            },
            "JS":
            {
                "Service":"View.JS",
                "Method":"Do"
            },
            "CSS":
            {
                "Service":"View.CSS",
                "Method":"Do"
            },
            "Meta":
            {
                "Service":"View.HTML.Meta",
                "Method":"Process"
            },
            "Breadcrumbs":
            {
                "Service":"View.HTML.Breadcrumbs",
                "Method":"Scan"
            },
            "No Remote":
            {
                "Service":"View.HTML.NoRemote",
                "Method":"Process"
            },
            "Locale URLs":
            {
                "Service":"View.HTML.LocalizedURLs",
                "Method":"Process"
            },
            "Places":
            {
                "Service":"View.HTML.Places",
                "Method":"Clean"
            },
            "Minify":
            {
                "Service":"View.HTML.Minify",
                "Method":"Process"
            },
            "I18N":
            {
                "Service":"View.HTML.Locale",
                "Method":"Process"
            }
        }
    }
}
