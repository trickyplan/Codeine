{
    "Links":
    {
        "/join":
        {
            "Service":"Entity.Create",
            "Method":"Do",
            "Call":
            {
                "Entity": "User"
            }
        },
        "/login":
        {
            "Service":"User.Login",
            "Method":"Do"
        },
        "/logout":
        {
            "Service":"User.Logout",
            "Method":"Do"
        },
        "/forgot":
        {
            "Service":"User.Reset",
            "Method":"Do"
        },
        "/changepwd":
        {
            "Service":"Entity.Update",
            "Method": "Do",
            "Call":
            {
                "Purpose": "Password",
                "OnlyTag": ["Password"],
                "Entity": "User",
                "Custom Layouts":
                {
                    "Update": "Update/Passwd"
                },
                "Where": "$Session.User.ID"
            }
        },
        "/cabinet":
        {
            "Service":"User.Cabinet",
            "Method":"Do"
        },
        "/profile":
        {
            "Service":"Entity.Update",
            "Call":
            {
                "Entity": "User",
                "Where": "$Session.User.ID"
            }
        },
        "/user/services":
        {
            "Service":"User.Services",
            "Method":"Do"
        },
        "/takeout":
        {
            "Service":"Entity.Delete",
            "Method":"Do",
            "Call":
            {
                "Entity": "User",
                "Where":
                {
                    "Service": "Session",
                    "Method": "Read",
                    "Call":
                    {
                        "Key": "User.ID"
                    }
                }
            }
        }
    }
}