{
    "Suites":
    {
        "CORS":
        {
            "OPTIONS":
            {
                "Run":
                {
                    "Service": "System.Interface.HTTP.CORS",
                    "Method": "Do",
                    "Call":
                    {
                        "HTTP":
                        {
                            "CORS":
                            {
                                "Enabled": true
                            },
                            "Method": "OPTIONS"
                        }
                    }
                },
                "Result":
                {
                    "Extract": "HTTP.Headers.Access-Control-Allow-Credentials:"
                },
                "Assert":
                {
                    "Equal": "true"
                }
            },
            "GET: Origin header passed":
            {
                "Run":
                {
                    "Service": "System.Interface.HTTP.CORS",
                    "Method": "Do",
                    "Call": 
                    {
                        "HTTP":
                        {
                            "Request":
                            {
                                "Headers":
                                {
                                    "Origin": "example.com"
                                }
                            },
                            "CORS":
                            {
                                "Enabled": true
                            },
                            "Method": "GET"
                        }
                    }
                },
                "Result":
                {
                    "Extract": "HTTP.Headers.Access-Control-Allow-Origin:"
                },
                "Assert":
                {
                    "Equal": "example.com"
                }
            },
            "GET":
            {
                "Run":
                {
                    "Service": "System.Interface.HTTP.CORS",
                    "Method": "Do",
                    "Call": 
                    {
                        "HTTP":
                        {
                            "CORS":
                            {
                                "Enabled": true
                            },
                            "Method": "GET"
                        }
                    }
                },
                "Result":
                {
                    "Extract": "HTTP.Headers.Access-Control-Allow-Origin:"
                },
                "Assert":
                {
                    "Equal": "*"
                }
            }
        }
    }
}
